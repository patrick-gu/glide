func fibonacci(n Int) Int {
    if eqInt(n, 0) {
        0
    } else if eqInt(n, 1) {
        1
    } else {
        add(fibonacci(sub(n, 1)), fibonacci(sub(n, 2)))
    }
}

func main() {
    printlnInt(fibonacci(0))
    printlnInt(fibonacci(1))
    printlnInt(fibonacci(2))
    printlnInt(fibonacci(3))
    printlnInt(fibonacci(4))
    printlnInt(fibonacci(5))
    printlnInt(fibonacci(6))
    printlnInt(fibonacci(7))
}
