func fibonacci(n Int) Int {
    if eqInt(n, 0) {
        0
    } else if eqInt(n, 1) {
        1
    } else {
        add(fibonacci(sub(n, 1)), fibonacci(sub(n, 2)))
    }
}

func main() {
    printInt(fibonacci(0))
    printInt(fibonacci(1))
    printInt(fibonacci(2))
    printInt(fibonacci(3))
    printInt(fibonacci(4))
    printInt(fibonacci(5))
    printInt(fibonacci(6))
    printInt(fibonacci(7))
}
